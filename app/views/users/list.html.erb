<div align="left">
<%= link_to("BACK", {:controller => 'users',:action => 'main'})%>
</div>
<div id="main">
<table class="listing" summary= "User data" border="1" align="center">
	<tr style="background-color:#00ff00">
		<th>Name</th>
		<th>Username</th>
		<th>Sex</th>
		<th>Dob</th>
		<th>Work</th>
		<th>Mobile</th>
		<th>Email</th>
		<th>Address</th>
		<th>Actions</th>
	</tr>
	<% @users.each do |user| %>
	<tr>
		<td><%= user.name %></td>
		<td><%= user.username %></td>
		<td><%= user.sex %></td>
		<td><%= user.dob %></td>
		<td><%= user.work %></td>
		<td><%= user.mobile %></td>
		<td><%= user.email %></td>
		<td><%= user.address %></td>
		<td>
			
				<%= link_to("Show", {:action => 'show', :id => user.id})%>
			
			
				<%= link_to("Edit", {:action => 'edit', :id => user.id})%>
			
			
				<%= link_to("Delete", {:action => 'delete', :id => user.id})%>	
			
		</td>
		
		
	</tr>
	<% end %>
</table>
</div>
